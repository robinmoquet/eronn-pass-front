{
  "name": "eronn-pass-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/yup": "^0.26.32",
    "formik": "^2.1.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "typescript": "~3.7.2",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "prettier": "1.19.1",
    "stylus": "^0.54.7"
  },
  "scripts": {
    "dev": "concurrently -k -n \"Stylus, React\" -p \"[{name}]\" -c \"yellow,blue\" \"npm run stylus:watch\" \"npm run react:start\"",
    "build": "npm run stylus:build && npm run react:build",
    "stylus:watch": "stylus ./styles/index.styl --include ./styles --out ./public/css --watch -m --compress",
    "stylus:build": "stylus ./styles/index.styl --include ./styles --out ./public/css --compress",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --config ./.prettierrc --write ./src/**/*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
